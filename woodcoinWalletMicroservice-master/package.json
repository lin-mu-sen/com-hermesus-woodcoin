{
   "name": "vignettes-microservices",
   "main": "dist/index.js",
   "license": "MIT",
   "scripts": {
      "build": "tsc --project tsconfig.build.json",
      "lint": "eslint --ext .ts,.js .",
      "lint:ci": "npm run lint && npm run prettier:ci",
      "prettier": "prettier 'src/**' --write --ignore-unknown",
      "prettier:ci": "prettier --check 'src/**' --ignore-unknown",
      "release": "standard-version",
      "start": "nodemon --exec ts-node src/index.ts ",
      "run": "kill $(lsof -ti tcp:5000) && ts-node src/index.ts",
      "test": "jest --detectOpenHandles --logHeapUsage",
      "test:ci": "npm run test -- --colors --coverage --ci",
      "test:coverage": "npm run test -- --coverage",
      "test:coverage:upload": "codecov -f coverage/*.json",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "docker": "docker logs vignettes-back_app_1 --follow",
      "docker:run": "docker ps",
      "postinstall": "patch-package"
   },
   "dependencies": {
      "@mtproto/core": "^6.0.1",
      "@nestjs/common": "^8.2.3",
      "@nestjs/core": "^8.2.3",
      "@nestjs/typeorm": "^8.0.2",
      "@types/notp": "^2.0.1",
      "base64-to-uint8array": "^1.0.0",
      "bcryptjs": "^2.4.3",
      "bip39": "^3.0.4",
      "bitcoin-core": "^3.0.0",
      "bitcore-mnemonic": "^8.25.30",
      "cron": "^1.8.2",
      "date-fns": "^2.27.0",
      "dotenv": "^10.0.0",
      "generate-sms-verification-code": "^1.0.5",
      "jimp": "^0.16.1",
      "jsonwebtoken": "^8.5.1",
      "moleculer": "^0.14.18",
      "moleculer-io": "^1.1.3",
      "moment": "^2.29.1",
      "mysql2": "^2.3.3",
      "node-fetch": "2.0",
      "notp": "^2.0.3",
      "onesignal-node": "^3.3.0",
      "otplib": "^12.0.1",
      "reflect-metadata": "^0.1.10",
      "session-jwt": "^2.0.6",
      "socket.io-redis": "^6.1.1",
      "socket.io-server": "^1.0.0",
      "sqlite3": "^5.0.2",
      "telegram": "^1.10.8",
      "thirty-two": "^1.0.2",
      "tslib": "^2.3.0",
      "twilio": "^3.71.2",
      "typeorm": "^0.2.41",
      "typeorm-encrypted": "^0.6.0",
      "uuid": "^8.3.2",
      "uuid-base58": "^1.2.0"
   },
   "devDependencies": {
      "@types/cron": "^1.7.3",
      "@types/jest": "^27.0.0",
      "@types/jsonwebtoken": "^8.5.6",
      "@types/node": "^8.0.29",
      "@types/uuid": "^8.3.3",
      "@typescript-eslint/eslint-plugin": "^4.28.4",
      "@typescript-eslint/parser": "^4.28.4",
      "codecov": "^3.8.3",
      "eslint": "^7.31.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-config-standard": "^16.0.3",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^5.1.0",
      "eslint-plugin-tsdoc": "^0.2.14",
      "jest": "^27.0.6",
      "lint-staged": "^11.0.1",
      "nodemon": "^2.0.12",
      "patch-package": "^6.4.7",
      "prettier": "^2.3.2",
      "standard-version": "^9.3.1",
      "ts-jest": "^27.0.3",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   }
}
